
{strip}


<div class="col-md-4">

    <scoa_reminders>

        <div class="ibox float-e-margins">

            <div class="ibox-title">
                <h5>Reminder</h5>

                <a class="text-success pull-right" href="javascript:void 0">
                    See all
                </a>

            </div>

            <div class="ibox-content inspinia-timeline">

                <div class="row" id="scoa_reminders">

                    <div class="row">
                        <div class="col-xs-12 no-borders text-center">
                            <h2>No data loaded</h2>
                        </div>
                    </div>


                </div>



            </div>


        </div>


    </scoa_reminders>

    <div class="ibox">
        <div class="ibox-title">
            <h5>Message us </h5>
            <div class="ibox-tools">
                <a class="collapse-link">
                    <i class="fa fa-chevron-up"></i>
                </a>

            </div>
        </div>

        <div class="ibox-content">


            <p>What do you want from us ?</p>

            <form role="form">
                <div class="form-group"><label>Subject</label>

                    <input type="text" required  class="form-control"></div>

                <div class="form-group"><label>Message</label>

                    <textarea style="margin: 0px 3px 0px 0px;min-width: 352px;min-height: 121px;" type="password"  class="form-control"></textarea>

                </div>

                <button class="btn btn-sm btn-primary btn-block" type="submit">

                    <i class="fa fa-send"></i>

                    <strong>Submit</strong>

                </button>

            </form>


        </div>
    </div>



</div>


{/strip}












